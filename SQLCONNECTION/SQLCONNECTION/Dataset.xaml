<Window x:Class="SQLCONNECTION.Dataset"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLCONNECTION"
        mc:Ignorable="d"
        Title="Executed" Height="2000" Width="1000">
    <StackPanel>
        <TextBlock  Text="name"  HorizontalAlignment="Left"   FontSize="34"  Margin="80,100,0,0"></TextBlock>
        <TextBox Height="50" x:Name="txtname" Width="500" ></TextBox>
        <TextBlock  Text="age"  HorizontalAlignment="Left"   FontSize="34"  Margin="80,100,0,0"></TextBlock>
        <TextBox Height="50" x:Name="txtage" Width="500" ></TextBox>
        <TextBlock  Text="email"  HorizontalAlignment="Left"   FontSize="34"  Margin="80,100,0,0"></TextBlock>
        <TextBox Height="50" x:Name="txtemail" Width="500" ></TextBox>

        <Button x:Name="txttest" Click="txttest_Click" Content="CLICK HERE" Height="50" FontSize="30" Width="500" Margin="40" ></Button>
        <Button x:Name="txtLOGIN" Click="txtLOGIN_Click" Content="LOGIN " Height="50" FontSize="30" Width="500" Margin="40" ></Button>

        <Button x:Name="txtset" Click="txtset_Click" Content="DATASET " Height="50" FontSize="30" Width="500" Margin="40" ></Button>
        <ComboBox x:Name="courselist" Height=" 70" Width="500" Text="COURSELIST " SelectionChanged="courselist_SelectionChanged" >
            
        </ComboBox>
    </StackPanel>

        

</Window>
